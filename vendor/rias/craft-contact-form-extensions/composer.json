{
    "name": "rias/craft-contact-form-extensions",
    "description": "Adds extensions to the Craft CMS contact form plugin.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft contact form extensions"
    ],
    "support": {
        "docs": "https://github.com/Rias500/craft-contact-form-extensions/blob/master/README.md",
        "issues": "https://github.com/Rias500/craft-contact-form-extensions/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rias",
            "homepage": "https://rias.be"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1",
        "craftcms/contact-form": "^2.1",
        "albertcht/invisible-recaptcha": "^1.8"
    },
    "autoload": {
        "psr-4": {
          "rias\\contactformextensions\\": "src/"
        }
    },
    "extra": {
        "name": "Contact Form Extensions",
        "handle": "contact-form-extensions",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/Rias500/craft-contact-form-extensions/master/CHANGELOG.md",
        "components": {
            "contactFormExtensionsService": "rias\\contactformextensions\\services\\ContactFormExtensionsService"
        },
        "class": "rias\\contactformextensions\\ContactFormExtensions"
    }
}
